{
    "compilerOptions": {
        "lib": [
            "es6",
            "dom"
        ],
        "module": "none",
        "moduleResolution": "node",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "strict": true,
        "target": "es5",
        "outDir": "../out"
    },
    "files": [
        "./main.ts"
    ]
}