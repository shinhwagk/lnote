{
    "name": "vscode-note",
    "displayName": "vscode-note",
    "description": "A simple note-taking extension.",
    "version": "0.9.128",
    "icon": "icon.png",
    "publisher": "shinhwagk",
    "preview": true,
    "engines": {
        "vscode": "^1.30.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/shinhwagk/vscode-note"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "note",
        "vscode"
    ],
    "license": "MIT",
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "vsnote-explorer",
                    "title": "vscode-note",
                    "icon": "images/note.svg"
                }
            ]
        },
        "views": {
            "vsnote-explorer": [
                {
                    "id": "domainExplorer",
                    "name": "domain"
                },
                {
                    "id": "filesExplorer",
                    "name": "files",
                    "when": "vscode-note.files-explorer"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "vscode-note.domain.create",
                    "group": "navigation",
                    "when": "view == domainExplorer"
                },
                {
                    "command": "vscode-note.domain.refresh",
                    "group": "navigation",
                    "when": "view == domainExplorer"
                },
                {
                    "command": "vscode-note.files.openTerminal",
                    "when": "view == filesExplorer",
                    "group": "navigation@3"
                },
                {
                    "command": "vscode-note.files.edit.open",
                    "when": "view == filesExplorer",
                    "group": "navigation@3"
                },
                {
                    "command": "vscode-note.files.close",
                    "group": "navigation@1",
                    "when": "view == filesExplorer"
                },
                {
                    "command": "vscode-note.files.refresh",
                    "group": "navigation@2",
                    "when": "view == filesExplorer"
                }
            ],
            "view/item/context": [
                {
                    "command": "vscode-note.domain.create",
                    "when": "view == domainExplorer",
                    "group": "0_domain"
                },
                {
                    "command": "vscode-note.domain.rename",
                    "when": "view == domainExplorer",
                    "group": "2_domain"
                },
                {
                    "command": "vscode-note.domain.move",
                    "when": "view == domainExplorer",
                    "group": "2_domain"
                },
                {
                    "command": "vscode-note.notes.create",
                    "when": "view == domainExplorer && viewItem == emptyNotes",
                    "group": "1_domain"
                },
                {
                    "command": "vscode-note.files.file.url.raw",
                    "when": "view == filesExplorer && viewItem == file"
                },
                {
                    "command": "vscode-note.files.file.url",
                    "when": "view == filesExplorer && viewItem == file"
                }
            ]
        },
        "configuration": [
            {
                "type": "object",
                "title": "vscode-note",
                "properties": {
                    "vscode-note.dbpath": {
                        "type": "string",
                        "default": "~/vscode-note",
                        "description": "note data location"
                    },
                    "vscode-note.github": {
                        "type": "string",
                        "description": "github remote"
                    }
                }
            }
        ],
        "commands": [
            {
                "command": "vscode-note.domain.create",
                "title": "New Domain",
                "icon": {
                    "light": "images/light/add.svg",
                    "dark": "images/dark/add.svg"
                }
            },
            {
                "command": "vscode-note.domain.move",
                "title": "Move Domain"
            },
            {
                "command": "vscode-note.domain.rename",
                "title": "Rename Domain"
            },
            {
                "command": "vscode-note.domain.refresh",
                "title": "Domain Refresh",
                "icon": {
                    "light": "images/light/refresh.svg",
                    "dark": "images/dark/refresh.svg"
                }
            },
            {
                "command": "vscode-note.files.close",
                "title": "Close",
                "icon": {
                    "light": "images/light/close.svg",
                    "dark": "images/dark/close.svg"
                }
            },
            {
                "command": "vscode-note.files.openTerminal",
                "title": "open terminal",
                "icon": {
                    "light": "images/light/terminal.svg",
                    "dark": "images/dark/terminal.svg"
                }
            },
            {
                "command": "vscode-note.notes.create",
                "title": "Create Notes"
            },
            {
                "command": "vscode-note.files.edit.open",
                "title": "open edit window",
                "icon": {
                    "light": "images/light/edit.svg",
                    "dark": "images/dark/edit.svg"
                }
            },
            {
                "command": "vscode-note.files.refresh",
                "title": "refresh",
                "icon": {
                    "light": "images/light/refresh.svg",
                    "dark": "images/dark/refresh.svg"
                }
            },
            {
                "command": "vscode-note.files.file.url.raw",
                "title": "github raw url"
            },
            {
                "command": "vscode-note.files.file.url",
                "title": "github url"
            }
        ]
    },
    "extensionPack": [
        "docsmsft.docs-markdown",
        "docsmsft.docs-preview"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "webpack --mode production --config webpack.config.ts --verbose --display-modules",
        "watch": "webpack --mode development --config webpack.config.ts --watch --info-verbosity verbose",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "jest"
    },
    "devDependencies": {
        "@babel/core": "^7.7.7",
        "@babel/preset-react": "^7.7.4",
        "@types/fs-extra": "^8.0.1",
        "@types/jest": "^24.0.24",
        "@types/node": "^12.12.21",
        "@types/object-path": "^0.11.0",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/rimraf": "^2.0.3",
        "@types/webpack": "^4.41.0",
        "babel-loader": "^8.0.6",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.4.0",
        "jest": "^24.9.0",
        "node-sass": "^4.13.0",
        "rimraf": "^3.0.0",
        "sass-loader": "^8.0.0",
        "style-loader": "^1.0.2",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.5.4",
        "typescript": "^3.7.3",
        "vscode": "^1.1.36",
        "webpack": "^4.41.4",
        "webpack-cli": "^3.3.10"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.26",
        "@fortawesome/free-solid-svg-icons": "^5.12.0",
        "@fortawesome/react-fontawesome": "^0.1.8",
        "fs-extra": "^8.1.0",
        "object-path": "^0.11.4",
        "react": "^16.12.0",
        "react-dom": "^16.12.0"
    }
}
